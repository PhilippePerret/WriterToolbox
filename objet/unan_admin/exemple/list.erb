<%
# Affichage de la liste des exemples
raise_unless_admin
%>
<%= Unan::titre_h1("Liste des exemples") %>
<%
ids_exemples = Unan::table_exemples.select(colonnes:[]).keys
%>
<% if ids_exemples.empty? %>
  <p>Aucun exemple pour le moment.</p>
<% else %>
  <ul id="exemples">
  <% ids_exemples.each do |eid| %>
    <%= Unan::Program::Exemple::get(eid).as_li %>
  <% end %>
  </ul>
<% end %>
<div class="right">
  <%= "CrÃ©er un nouvel exemple".in_a(href:"exemple/edit?in=unan_admin") %>
</div>
